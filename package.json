{
  "name": "consorcify",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "front",
    "back"
  ],
  "scripts": {
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "dev": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "dev:front": "npm --prefix front run dev",
    "dev:back": "npm --prefix back run start:dev"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
